let b=55;
let r1 = 9;
let r2 = -2;
let input = document.querySelector("#reply1");

for(let q = 0; r1*q+r2 < b; q++){
	let a = b*q+r1*q+r2;
	if(a >= 0)
		input.value += b*q+r1*q+r2 + ",";
}


let b=37;
let r1 = 3;
let r2 = -4;
let input = document.querySelector("#reply1");

for(let q = 0; r1*Math.pow(q,2)+r2 < b; q++){
	let a = b*q+r1*Math.pow(q,2)+r2;
	if(a >= 0)
		input.value += a + ",";
}